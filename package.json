{
  "name": "boilerplate-cypress-alurereport",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "tests:Headless": "npx cypress run",
    "open:Cypress": "npx cypress open",
    "generate:AllureReport":"allure generate allure-results --clean -o allure-report && rimraf ./allure-results",
    "open:AllureReport":"allure open  allure-report",
    "clean:AllureResults": "rimraf ./allure-results",
    "tests-on-Chrome:Hedless": "cypress run --browser chrome",
    "tests-on-Firefox:Hedless": "cypress run --browser firefox",
    "tests-on-Edge:Hedless": "cypress run --browser edge",
    "tests-on-Electron:Hedless": "cypress run --browser electron"
  },
 
  "author": "",
  "license": "ISC",
  "dependencies": {
    "allure-commandline": "^2.18.1",
    "cypress": "^10.10.0",
    "mocha-allure-reporter": "^1.4.0"
  }
}
